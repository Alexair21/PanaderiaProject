{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ],
    "build": {
      "env": {
        "COMPOSER_CACHE_DIR": ".vercel/cache"
      },
      "commands": {
        "install": "composer install"
      }
    }
  }
